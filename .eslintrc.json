{
  "extends": ["next", "next/typescript", "prettier"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn",
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "alphabetize": {
          "caseInsensitive": true,
          "order": "asc",
          "orderImportKind": "asc"
        },
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ]
      }
    ]
  }
}
